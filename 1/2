a = int(input())
if a % 2 == 0:
    print("Данное число четное")
else:
    print("Данное число не является четным")


a = int(input())
b = int(input())
if a > b:
    print(a)
else:
    print(b)

a = int(input())
b = int(input())
if (a % 2 == 0 and b % 2 > 0) or (b % 2 == 0 and a % 2 > 0):
    print("YES")
else:
    print("NO")
